{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "assignees": [
    "@john"
  ],
  "packageRules": [
  {
    "matchDatasources": ["docker"],
    "matchPackageNames": [
        "gitlab/gitlab-runner:alpine(?<currentValue>3\\.\\d+)",
        "gitlab/gitlab-runner:alpine(?<depName>3\\.\\d+)-(?<currentValue>[a-zA-Z0-9]+)" 
      ],
    "versioning": "loose"
  }
  ]
}
 